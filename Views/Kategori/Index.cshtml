@model IEnumerable<FSWEBAPP.Models.Kategori>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.ActionLink($"{item.KategoriID} - Edit", "Edit", new { id = item.KategoriID }) |
